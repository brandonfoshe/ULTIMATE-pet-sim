<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" prefHeight="300.0" prefWidth="250.0" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.petsimulator.SelectionController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <children>
      <Button mnemonicParsing="false" onAction="#dogSelection" text="Dog" />
      <Button mnemonicParsing="false" onAction="#catSelection" text="Cat" />
      <Label text="More to come soon!" />
      <Button mnemonicParsing="false" onAction="#BackMainMenu" text="Back" />
   </children>
</VBox>
